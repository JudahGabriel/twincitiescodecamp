{
  "Logging": {
    "IncludeScopes": true,
    "StructuredLogMaxOccurrences": 20,
    "LogLevel": {
      "Default": "Debug",
      "System": "Error",
      "Microsoft": "Error"
    }
  },

  "AllowedHosts": "*",

  "pwa": {
    "strategy": "minimal" // so that dev changes show up immediately
  }
}
